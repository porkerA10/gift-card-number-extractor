{
  "CreationDate": "2018-1-23",
  "Commands": [
    {
      "Command": "click",
      "Target": "/descendant::span[@autoid='_lvv_g' and text()[contains(.,'Your order number and claim link')]][${!LOOP}]",
      "Value": ""
    },
    {
      "Command": "store",
      "Target": "0",
      "Value": "i"
    },
    {
      "Command": "storeEval",
      "Target": "document.evaluate(\"count(/descendant::a[text()[contains(.,'View My Code')]])\", document).numberValue",
